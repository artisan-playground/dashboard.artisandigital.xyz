{
  "name": "artisan-dashboard-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && GENERATE_SOURCEMAP=false react-scripts build",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/tailwind-generated.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/tailwind-generated.css --env production"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@pathofdev/react-tag-input": "^1.0.7",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-linkify": "^1.0.0",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.8.5",
    "apollo-upload-client": "^14.1.3",
    "bizcharts": "^4.0.15",
    "cssnano": "^4.1.10",
    "dayjs": "^1.9.6",
    "easy-peasy": "^4.0.1",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-linkify": "^1.0.0-alpha",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "reactjs-localstorage": "^1.0.1",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
