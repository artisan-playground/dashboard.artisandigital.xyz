{
  "name": "artisan-dashboard-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css --env production"
  },
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@pathofdev/react-tag-input": "^1.0.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-linkify": "^1.0.0",
    "@types/react-router-dom": "^5.1.5",
    "antd": "^4.5.2",
    "antd-img-crop": "^3.10.0",
    "autoprefixer": "^9.8.5",
    "axios": "^0.19.2",
    "cssnano": "^4.1.10",
    "easy-peasy": "^3.3.1",
    "graphql": "^15.3.0",
    "moment": "^2.27.0",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "react-highlight-words": "^0.16.0",
    "react-items-carousel": "^2.8.0",
    "react-linkify": "^1.0.0-alpha",
    "react-multi-carousel": "^2.5.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.6.0",
    "typescript": "^3.9.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
