{
  "name": "artisan-dashboard-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css --env production"
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@pathofdev/react-tag-input": "^1.0.7",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.0",
    "@types/react-linkify": "^1.0.0",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.7.0",
    "antd-img-crop": "^3.12.0",
    "autoprefixer": "^10.0.1",
    "axios": "^0.20.0",
    "cssnano": "^4.1.10",
    "easy-peasy": "^3.3.1",
    "graphql": "^15.3.0",
    "moment": "^2.29.1",
    "postcss-cli": "^8.1.0",
    "react": "^16.14.0",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.14.0",
    "react-highlight-words": "^0.16.0",
    "react-items-carousel": "^2.8.0",
    "react-linkify": "^1.0.0-alpha",
    "react-multi-carousel": "^2.5.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.9.6",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
