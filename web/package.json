{
  "name": "artisan-dashboard-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run watch:css && craco start",
    "build": "npm run build:css && craco build",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/tailwind-generated.css",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/tailwind-generated.css --env production"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@ckeditor/ckeditor5-build-classic": "^24.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.0",
    "@craco/craco": "^6.0.0",
    "@turf/circle": "^6.0.1",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.8.5",
    "apollo-upload-client": "^14.1.3",
    "bizcharts": "^4.0.15",
    "craco-less": "^1.17.1",
    "cssnano": "^4.1.10",
    "dayjs": "^1.9.6",
    "easy-peasy": "^4.0.1",
    "html-react-parser": "^1.2.1",
    "mapbox-gl": "^2.0.0",
    "postcss": "^8.1.10",
    "postcss-cli": "^8.3.0",
    "prop-types": "^15.7.2",
