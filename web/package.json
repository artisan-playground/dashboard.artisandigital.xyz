    "react-dom": "^17.0.1",
    "react-mapbox-gl": "^5.1.1",
    "react-mapbox-wrapper": "^2.6.1",
    "react-mobile-devices": "^0.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "reactjs-localstorage": "^1.0.1",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
